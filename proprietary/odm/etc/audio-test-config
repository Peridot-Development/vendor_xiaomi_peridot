#capture_main-mic
#enable_main-mic
enable
TX DEC1 MUX:SWR_MIC
TX SMIC MUX1:SWR_MIC0
TX_AIF1_CAP Mixer DEC1:1
ADC1 ChMap:SWRM_TX1_CH1
ADC1_MIXER Switch:1

#disable_main-mic
disable
TX DEC1 MUX:MSM_DMIC
ADC1 ChMap:ZERO
TX SMIC MUX1:ZERO
TX_AIF1_CAP Mixer DEC1:0
ADC1_MIXER Switch:0

#capture_top-mic
#enable_top-mic
enable
TX DEC1 MUX:SWR_MIC
TX SMIC MUX1:SWR_MIC5
TX_AIF1_CAP Mixer DEC1:1
ADC3 ChMap:SWRM_TX2_CH2
ADC3_MIXER Switch:1

#disable_top-mic
disable
