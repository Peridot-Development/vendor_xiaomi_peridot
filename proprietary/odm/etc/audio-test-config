TX SMIC MUX1:ZERO
TX_AIF1_CAP Mixer DEC1:0
ADC3 ChMap:ZERO
ADC3_MIXER Switch:0


#capture_us
enable
TX DEC4 MUX:SWR_MIC
TX SMIC MUX4:SWR_MIC5
TX_AIF2_CAP Mixer DEC4:1
ADC3 ChMap:SWRM_TX2_CH2
ADC3_MIXER Switch:1
ADC3 Volume:12

disable
ADC3_MIXER Switch:0
ADC3 ChMap:ZERO
TX_AIF2_CAP Mixer DEC4:0
TX SMIC MUX4:ZERO
TX DEC4 MUX:MSM_DMIC
ADC3 Volume:6


