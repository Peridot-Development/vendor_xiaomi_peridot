HPHR_RDAC Switch:1
disable

#teardown_loopback
enable
TX DEC1 MUX:MSM_DMIC
TX SMIC MUX1:ZERO
TX_AIF1_CAP Mixer DEC1:0
TX DEC0 MUX:ZERO
TX SMIC MUX0:ZERO
ADC1 ChMap:ZERO
ADC2 ChMap:ZERO
ADC3 ChMap:ZERO
ADC4 ChMap:ZERO
ADC1_MIXER Switch:0
ADC2_MIXER Switch:0
ADC3_MIXER Switch:0
ADC4_MIXER Switch:0
TX0 MODE:ADC_LP
TX2 MODE:ADC_LP
TX3 MODE:ADC_LP
HDR12 MUX:NO_HDR12
ADC2 MUX:INP2
ADC4 MUX:INP5
HPHL Volume:20
HPHR Volume:20
ADC1 Volume:6
ADC2 Volume:6
ADC3 Volume:6
ADC4 Volume:6
RX INT0_1 MIX1 INP0:ZERO
RX INT1_1 MIX1 INP0:ZERO
RX INT0 DEM MUX:NORMAL_DSM_OUT
RX INT1 DEM MUX:NORMAL_DSM_OUT
RX_HPH_PWR_MODE:ULP
RX HPH Mode:CLS_H_ULP
RX_COMP1 Switch:0
RX_COMP2 Switch:0
HPHL_COMP Switch:0
HPHR_COMP Switch:0
HPHL Volume:18
HPHR Volume:18
HPHL_RDAC Switch:0
HPHR_RDAC Switch:0
aw_dev_0_prof:Music
aw_dev_1_prof:Music
aw_dev_0_switch:Disable
aw_dev_1_switch:Disable
aw882xx_spk_sw_gpio_switch:0
FSM_SW_GPIO:0
FSM_Stop:1
FSM_Volume:255
FSM_Scene:0
disable


