service vendor.vppservice /vendor/bin/vppservice
    class hal
    user mediacodec
    group camera system media

    # AIDL interface
    interface aidl vendor.qti.hardware.vpp.IVppService/default

    # don't auto restart
    oneshot

    # start only when requested (uncomment below to disable service at boot)
    # disabled
